@**定義畫面上的狀態值*@
@functions {
    //估驗單與費用申請單狀態
    public static string shwoStatus(string status)
    {
        string result = "";
        switch (status)
        {
            case "0":
                result = "退件";
                break;
            case "1": //財務建立的資料
                result = "已估驗";
                break;
            case "10":
                result = "草稿";
                break;
            case "20":
                result = "審核中";
                break;
            case "30":
                result = "審核通過";
                break;
            case "40":
                result = "財審通過";
                break;
            case "50":
                result = "已付款";
                break;
            default:
                result = "有誤";
                break;
        }
        return "<label>" + result + "</label>";
    }
}
