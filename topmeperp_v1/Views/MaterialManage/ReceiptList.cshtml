@model IEnumerable<topmeperp.Models.PRFunction>
@{
    ViewBag.Title = "驗收明細";
    Layout = "~/Views/Shared/_MenuLayout.cshtml";
}
<div class="container">
    <h3>已驗收明細 : </h3>
    <input type="button" class="btn btn-success" onclick="history.back()" value="回上一頁">
    <div class="row">
        <table class="table table-bordered">
            <thead>
                <tr>
                    <th>項次</th>
                    <th>驗收日期</th>
                    <th>編號</th>
                    <th>供應商</th>
                </tr>
            </thead>
            <tbody>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@item.NO</td>
                        <td>@item.CREATE_DATE</td>
                        <td><a href="/MaterialManage/SingleRP?id=@item.PR_ID&parentId=@item.PARENT_PR_ID&prjid=@item.PROJECT_ID">@item.PR_ID</a></td>
                        <td>@item.SUPPLIER_ID</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>


