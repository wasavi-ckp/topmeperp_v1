
<!-- Modal content-->
<div class="modal-content">
    <form id="formSubject" name="formSubject" action="addSubject" method="post">
        <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal">&times;</button>
            <h4 class="modal-title">財務科目資料</h4>
        </div>
        <div class="modal-body form-group">
            <div id="subjectid4new" class="row form-group"><div class="col-md-4">項目代碼</div><div class="col-md-8"><input type="text" id="new_subject_id" name="new_subject_id" class="form-control" /></div></div>
            <div id="subjectid4now" class="row form-group"><div class="col-md-4">項目代碼</div><div class="col-md-8"><input type="text" id="subject_id" name="subject_id" class="form-control" readonly /></div></div>
            <div class="row form-group"><div class="col-md-4">項目名稱</div><div class="col-md-8"><input type="text" id="subject_name" name="subject_name" class="form-control" /></div></div>
            <div class="row form-group">
                <div class="col-md-4">項目類別</div><div class="col-md-8">
                    @Html.DropDownList("categorys", null, String.Empty, new { @class = "form-control" })
                </div>
            </div>
        </div>
        <div class="modal-footer">
            <button type="button" class="btn btn-primary" data-dismiss="modal">關閉</button>
            <button type="submit" class="btn btn-default" data-dismiss="modal" id="saveSubject">儲存</button>
        </div>
    </form>
</div>
