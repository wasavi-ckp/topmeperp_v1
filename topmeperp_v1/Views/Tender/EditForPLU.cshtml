@using topmeperp.Models
@model TND_MAP_PLU
@{
    ViewBag.Title = "備標作業-修改給排水發包數量";
    Layout = "~/Views/Shared/_MenuLayout.cshtml";
}
<div class="col-md-12">
    <!-- Reg-Form new { enctype = "multipart/form-data" } -->
    @using (Html.BeginForm("EditForFW", "Tender", new { ReturnUrl = ViewBag.ReturnUrl }, FormMethod.Post, new { enctype = "multipart/form-data" }))
    {
        <div class="row">
            <br />
            <div class="row col-md-12" style="padding-bottom:30px;">
                <div class="col-md-4">
                    <h3>給排水發包數量</h3>
                </div>
                <div class="col-md-8" style="padding-top:10px;">
                    <input type="button" class="btn btn-success" onclick="history.back()" value="回上一頁">
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-8">
                <div class="row ">
                    @Html.HiddenFor(m => m.PLU_ID)
                    @Html.HiddenFor(m => m.PROJECT_ID)
                    <label class="col-md-2 control-label">項次 : </label>
                    <div class="col-md-10">
                        <label class="input">
                            @Html.TextBoxFor(m => m.EXCEL_ITEM, new { @class = "form-control" })
                        </label>
                    </div>

                    <label class="col-md-2 control-label">圖號 : </label>
                    <div class="col-md-10">
                        <label class="input">
                            @Html.TextBoxFor(m => m.MAP_NO, new { @class = "form-control" })
                        </label>
                    </div>

                    <label class="col-md-2 control-label">棟別 : </label>
                    <div class="col-md-10">
                        <label class="input">
                            @Html.TextBoxFor(m => m.BUILDING_NO, new { @class = "form-control" })
                        </label>
                    </div>

                    <label class="col-md-2 control-label">一次側位置 : </label>
                    <div class="col-md-10">
                        <label class="input">
                            @Html.TextBoxFor(m => m.PRIMARY_SIDE, new { @class = "form-control" })
                        </label>
                    </div>

                    <label class="col-md-2 control-label">一次側名稱 : </label>
                    <div class="col-md-10">
                        <label class="input">
                            @Html.TextBoxFor(m => m.PRIMARY_SIDE_NAME, new { @class = "form-control" })
                        </label>
                    </div>

                    <label class="col-md-2 control-label">二次側名稱 : </label>
                    <div class="col-md-10">
                        <label class="input">
                            @Html.TextBoxFor(m => m.SECONDARY_SIDE, new { @class = "form-control" })
                        </label>
                    </div>

                    <label class="col-md-2 control-label">二次側位置 : </label>
                    <div class="col-md-10">
                        <label class="input">
                            @Html.TextBoxFor(m => m.SECONDARY_SIDE_NAME, new { @class = "form-control" })
                        </label>
                    </div>

                    <label class="col-md-2 control-label">管材名稱 : </label>
                    <div class="col-md-10">
                        <label class="input">
                            @Html.TextBoxFor(m => m.PIPE_NAME, new { @class = "form-control" })
                        </label>
                    </div>

                    <label class="col-md-2 control-label">管數/組 : </label>
                    <div class="col-md-10">
                        <label class="input">
                            @Html.TextBoxFor(m => m.PIPE_COUNT_SET, new { @class = "form-control" })
                        </label>
                    </div>

                    <label class="col-md-2 control-label">管組數 : </label>
                    <div class="col-md-10">
                        <label class="input">
                            @Html.TextBoxFor(m => m.PIPE_SET_QTY, new { @class = "form-control" })
                        </label>
                    </div>
                    <label class="col-md-2 control-label">管長 : </label>
                    <div class="col-md-10">
                        <label class="input">
                            @Html.TextBoxFor(m => m.PIPE_LENGTH, new { @class = "form-control" })
                        </label>
                    </div>

                    <label class="col-md-2 control-label">管總長 : </label>
                    <div class="col-md-10">
                        <label class="input">
                            @Html.TextBoxFor(m => m.PIPE_TOTAL_LENGTH, new { @class = "form-control" })
                        </label>
                    </div>
                    @Html.HiddenFor(m => m.CREATE_ID)
                    @Html.HiddenFor(m => m.CREATE_DATE)
                </div>
            </div>
            <div class="col-md-4 alert-info">@Html.Raw(ViewBag.result)</div>
        </div>
        <br />
        <div class="row">
            <div class="" col-md-12">
                <input type="submit" value="儲存" class="btn btn-default" />
            </div>
        </div>
    }
</div>



