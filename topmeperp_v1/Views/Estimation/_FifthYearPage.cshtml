</div>

<script>
    $(document).ready(function () {
        var total1 = 0;
        var sum1 = 0;
        var total2 = 0;
        var sum2 = 0;
        var total3 = 0;
        var sum3 = 0;
        var total4 = 0;
        var sum4 = 0;
        var total5 = 0;
        var sum5 = 0;
        var total6 = 0;
        var sum6 = 0;
        var total7 = 0;
        var sum7 = 0;
        var total8 = 0;
        var sum8 = 0;
        var total9 = 0;
        var sum9 = 0;
        var total0 = 0;
        var sum10 = 0;
        var total11 = 0;
        var sum11 = 0;
        var total12 = 0;
        var sum12 = 0;
        var sum13 = 0;
        $('table > tbody > tr').each(function () {
            var atm1 = 0; var atm2 = 0; var atm3 = 0;
            var atm4 = 0; var atm5 = 0; var atm6 = 0;
            var atm7 = 0; var atm8 = 0; var atm9 = 0;
            var atm10 = 0; var atm11 = 0; var atm12 = 0;

            if ($.trim($(this).find("#janAmt5").val()) != "") {
                atm1 = parseInt($.trim($(this).find("#janAmt5").val().replace(",", "")));
            }
            if ($.trim($(this).find("#febAmt5").val()) != "") {
                atm2 = parseInt($.trim($(this).find("#febAmt5").val().replace(",", "")));
            }
            if ($.trim($(this).find("#marAmt5").val()) != "") {
                atm3 = parseInt($.trim($(this).find("#marAmt5").val().replace(",", "")));
            }
            if ($.trim($(this).find("#aprAmt5").val()) != "") {
                atm4 = parseInt($.trim($(this).find("#aprAmt5").val().replace(",", "")));
            }
            if ($.trim($(this).find("#mayAmt5").val()) != "") {
                atm5 = parseInt($.trim($(this).find("#mayAmt5").val().replace(",", "")));
            }
            if ($.trim($(this).find("#junAmt5").val()) != "") {
                atm6 = parseInt($.trim($(this).find("#junAmt5").val().replace(",", "")));
            }
            if ($.trim($(this).find("#julAmt5").val()) != "") {
                atm7 = parseInt($.trim($(this).find("#julAmt5").val().replace(",", "")));
            }
            if ($.trim($(this).find("#augAmt5").val()) != "") {
                atm8 = parseInt($.trim($(this).find("#augAmt5").val().replace(",", "")));
            }
            if ($.trim($(this).find("#sepAmt5").val()) != "") {
                atm9 = parseInt($.trim($(this).find("#sepAmt5").val().replace(",", "")));
            }
            if ($.trim($(this).find("#octAmt5").val()) != "") {
                atm10 = parseInt($.trim($(this).find("#octAmt5").val().replace(",", "")));
            }
            if ($.trim($(this).find("#novAmt5").val()) != "") {
                atm11 = parseInt($.trim($(this).find("#novAmt5").val().replace(",", "")));
            }
            if ($.trim($(this).find("#decAmt5").val()) != "") {
                atm12 = parseInt($.trim($(this).find("#decAmt5").val().replace(",", "")));
            }

            total1 = atm1; total2 = atm2; total3 = atm3;
            total4 = atm4; total5 = atm5; total6 = atm6;
            total7 = atm7; total8 = atm8; total9 = atm9;
            total10 = atm10; total11 = atm11; total12 = atm12;

            sum1 += total1; sum2 += total2; sum3 += total3;
            sum4 += total4; sum5 += total5; sum6 += total6;
            sum7 += total7; sum8 += total8; sum9 += total9;
            sum10 += total10; sum11 += total11; sum12 += total12;
            sum13 += total1 + total2 + total3 + total4 + total5 + total6 + total7 + total8 + total9 + total10 + total11 + total12;

        });
        $("#janVTotal_5").val(numeral(sum1).format('0,0'));
        $("#febVTotal_5").val(numeral(sum2).format('0,0'));
        $("#marVTotal_5").val(numeral(sum3).format('0,0'));
        $("#aprVTotal_5").val(numeral(sum4).format('0,0'));
        $("#mayVTotal_5").val(numeral(sum5).format('0,0'));
        $("#junVTotal_5").val(numeral(sum6).format('0,0'));
        $("#julVTotal_5").val(numeral(sum7).format('0,0'));
        $("#augVTotal_5").val(numeral(sum8).format('0,0'));
        $("#sepVTotal_5").val(numeral(sum9).format('0,0'));
        $("#octVTotal_5").val(numeral(sum10).format('0,0'));
        $("#novVTotal_5").val(numeral(sum11).format('0,0'));
        $("#decVTotal_5").val(numeral(sum12).format('0,0'));
        $("#VTotal_5").val(numeral(sum13).format('0,0'));
