  <!--log4net confiuration section by ph-->
  <log4net debug="true">
    <root>
      <level value="DEBUG" />
      <appender-ref ref="RollingFile" />
      <appender-ref ref="ScheduleFile" />
    </root>
    <!--一般log-->
    <appender name="RollingFile" type="log4net.Appender.RollingFileAppender">
      <filter type="log4net.Filter.LoggerMatchFilter">
        <loggerToMatch value="topmeperp.Schedule" />
        <acceptOnMatch value="false" />
      </filter>
      <file type="log4net.Util.PatternString" value="D:/LogFiles/Log-%date{yyyyMMdd}/tompep.log" />
      <!--<lockingModel type="log4net.Appender.FileAppender+MinimalLock" />-->
      <appendToFile value="true" />
      <rollingStyle value="Composite" />
      <maxSizeRollBackups value="-1" />
      <maximumFileSize value="50MB" />
      <staticLogFileName value="true" />
      <datePattern value="yyyy-MM-dd" />
      <countDirection value="1" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level %logger.%method - %message%newline" />
      </layout>
    </appender>
    <!--排程作業log-->
    <appender name="ScheduleFile" type="log4net.Appender.RollingFileAppender">
      <filter type="log4net.Filter.LoggerMatchFilter">
        <loggerToMatch value="topmeperp.Schedule" />
        <acceptOnMatch value="true" />
      </filter>
      <filter type="log4net.Filter.LevelRangeFilter">
        <levelMin value="ERROR" />
        <levelMax value="INFO" />
      </filter>
      <file type="log4net.Util.PatternString" value="D:/LogFiles/Log-%date{yyyyMMdd}/tompep_ScheduleFile.log" />
      <!--<lockingModel type="log4net.Appender.FileAppender+MinimalLock" />-->
      <appendToFile value="true" />
      <rollingStyle value="Composite" />
      <maxSizeRollBackups value="-1" />
      <maximumFileSize value="50MB" />
      <staticLogFileName value="true" />
      <datePattern value="yyyy-MM-dd" />
      <countDirection value="1" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level %logger.%method - %message%newline" />
      </layout>
    </appender>
  </log4net>
